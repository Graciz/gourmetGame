package goumertGraci.model;

import java.util.ArrayList;
import java.util.Arrays;
import java.util.List;

public class KnowledgeBase {
	
	List<Dishes> sweetDishes;
	List<Dishes> saltyDishes;
	
	public KnowledgeBase() {
		this.sweetDishes = new ArrayList<Dishes>();
		this.saltyDishes = new ArrayList<Dishes>();
	}


	public void initialize() {
		
		this.sweetDishes = Arrays.asList(new Dishes(null,"Bolo de Chocolate" ));  
		new Dishes("massa", "Lasanha");
		;
	}
	
	

}
